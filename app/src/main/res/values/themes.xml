<resources>
    <!-- Base application theme. -->
    <style name="Theme.Ine" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/InePrimary</item>
        <item name="colorPrimaryAlpha">@color/InePrimaryAlpha</item>
        <item name="colorPrimaryDark">@color/InePrimaryDark</item>
        <item name="colorAccent">@color/IneAccent</item>
        <item name="colorBackground">@color/Background</item>
        <!-- Status bar color. -->
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:statusBarColor">@color/Background</item>
        <!-- Customize your theme here. -->
    </style>

